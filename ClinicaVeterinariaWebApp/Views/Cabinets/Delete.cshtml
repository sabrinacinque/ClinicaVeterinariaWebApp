@model ClinicaVeterinariaWebApp.Models.Cabinet

@{
    ViewData["Title"] = "Cancella";
}

<h1>Cancella</h1>

<h3>Sei sicuro di voler cancellare l'armadietto?</h3>
<div>
    <h4>Armadietto</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Codice
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Code)
        </dd>
    </dl>

    <form asp-action="Delete">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Cancella" class="btn btn-danger" /> |
        <a asp-action="Index">Ritorna alla lista</a>
    </form>
</div>
